{
  "name": "breaking-bad-voting-graphql",
  "version": "1.0.0",
  "description": "Node server in GraphQL that will allow us to make a voting system in real time with graphics about who is our favorite Breaking Bad character ",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc -p . && ncp src/schema build/schema",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon 'src/server.ts' --exec 'ts-node' src-server.ts -e ts,graphql,json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphql-course/breaking-bad-voting-graphql.git"
  },
  "keywords": [
    "graphql",
    "breaking-bad",
    "characters-breaking-bad",
    "voting-characters-breaking-bad",
    "suscriptions-graphql"
  ],
  "author": "Anartz Mugika <mugan86@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphql-course/breaking-bad-voting-graphql/issues"
  },
  "homepage": "https://github.com/graphql-course/breaking-bad-voting-graphql#readme",
  "dependencies": {
    "apollo-server-express": "^2.6.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "graphql-import-node": "0.0.4",
    "graphql-playground-middleware-express": "^1.7.12",
    "graphql-tools": "^4.0.4",
    "http": "0.0.0",
    "lodash": "^4.17.11",
    "ncp": "^2.0.0",
    "subscriptions-transport-ws": "^0.9.16",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/graphql": "^14.2.0",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.8"
  },
  "engines": {
      "node": "10.15.3"
  }
}
